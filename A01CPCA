#include<bits/stdc++.h>

using namespace std;

int main()
{
    ios::sync_with_stdio(false);
    int a,b,c;
    while(1)
    {
        cin>>a>>b>>c;
        if(a==0 && b==0 && c==0)
            break;
        if(a+c==2*b && b!=a)
            cout<<"AP "<<c+(c-b)<<"\n";
        else if(a*c==b*b && b!=0)
            cout<<"GP "<<c*(c/b)<<"\n";
    }

    return 0;
}
